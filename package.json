{
  "name": "noaa-d3",
  "version": "1.0.0",
  "description": "NOAA data with d3.js",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "lint": "eslint *.js",
    "lint:fix": "eslint *.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "d3": "^7.9.0",
    "duckdb": "^1.3.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!eslint.config.js",
      "!jest.config.js"
    ],
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}
